<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.railway.basicmap.original.repository.BureauDicMapper">
    
    <select id="getBureauDictMap" resultType="com.railway.basicmap.original.entity.BureauDic">
        select ljdm,ljjc,ljpym,ljqc
